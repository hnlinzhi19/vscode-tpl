{
  "name": "vscode-tpl",
  "displayName": "vscode-tpl",
  "description": "linz tpl arttemplate",
  "version": "0.0.1",
  "publisher": "linz-tpl",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:linz.tpl",
    "onCommand:linz.cssImg"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "linz.tpl",
        "title": "linz tpl"
      },
      {
        "command": "linz.cssImg",
        "title": "linz cssImg"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorLangId == html",
          "command": "linz.tpl"
        },
        {
          "when": "editorLangId == scss",          
          "command": "linz.cssImg"
        },
        {
          "when": "editorLangId == css",          
          "command": "linz.cssImg"
        },
        {
          "when": "editorLangId == less",          
          "command": "linz.cssImg"
        },
        {
          "when": "editorLangId == vue",          
          "command": "linz.cssImg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "globby": "^6.1.0",
    "jimp": "^0.2.27",
    "lodash": "^4.17.4",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.1.0"
  }
}
